{
  "name": "@rippledesk/wp",
  "version": "1.0.0",
  "description": "Rippledesk Wordpress plugin - Modern AI phone for SMBs",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build --config webpack.config.js",
    "build:prod": "ENVIRONMENT=PRODUCTION wp-scripts build --config webpack.config.js",
    "dev": "wp-scripts start --config webpack.config.dev.js",
    "codegen": "dotenv -e .env.local graphql-codegen --config codegen.ts",
    "build:translations:files": "tsc --build tsconfig.json",
    "create:translate:files": "wp i18n make-pot . languages/rippledesk.pot --allow-root --include=rippledesk.php,wp-translations-dist/src --ignore-domain",
    "make:translations:json": "wp i18n make-json languages --no-purge"
  },
  "keywords": [
    "Rippledesk",
    "Phone number",
    "Free calls"
  ],
  "author": "theophilus@rippledesk.com <Theophilus Annor>",
  "license": "GPL-2.0-only",
  "type": "commonjs",
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.7",
    "@graphql-codegen/client-preset": "4.8.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript-operations": "4.6.1",
    "@graphql-codegen/typescript-react-apollo": "4.3.3",
    "@graphql-codegen/typescript-resolvers": "4.5.1",
    "@types/apollo-upload-client": "17.0.3",
    "@types/crypto-js": "^4",
    "@types/howler": "^2",
    "@types/react-color": "^2",
    "@wordpress/scripts": "30.21.0",
    "dotenv-cli": "^10.0.0",
    "dotenv-webpack": "8.1.1",
    "ts-loader": "9.5.2",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@intercom/messenger-js-sdk": "^0.0.15",
    "@lexical/react": "^0.34.0",
    "@mui/icons-material": "7.3.1",
    "@mui/joy": "5.0.0-beta.52",
    "@mui/material": "7.3.1",
    "@mui/x-data-grid": "^8.10.2",
    "@rippledesk/ui": "^1.0.52",
    "@rippledesk/ui-utils": "^0.0.9",
    "@sentry/react": "^10.5.0",
    "@tanem/react-nprogress": "^5.0.55",
    "@twilio/voice-sdk": "2.15.0",
    "@uiw/react-color": "^2.8.0",
    "@wordpress/api-fetch": "7.28.0",
    "@wordpress/components": "^30.1.0",
    "@wordpress/dom-ready": "4.28.0",
    "@wordpress/element": "6.28.0",
    "@wordpress/i18n": "^6.1.0",
    "apollo-upload-client": "17.0.0",
    "apollo3-cache-persist": "^0.15.0",
    "axios-hooks": "5.1.1",
    "axios-retry": "4.5.0",
    "chart.js": "4.5.0",
    "crypto-js": "^4.2.0",
    "emoji-picker-react": "^4.13.2",
    "filesize": "11.0.2",
    "howler": "2.2.4",
    "lexical-beautiful-mentions": "^0.1.47",
    "localforage": "^1.10.0",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "react-chartjs-2": "5.3.0",
